setwd("~/Documents/my_gits/Triplet_rooting/analyses/Bio_Data/")
require("ggplot2")
d = read.table("dmc.dat",header = T)
ggplot(d,aes(x=method,y=dmc,group=method,fill=method)) +
geom_boxplot(outlier.size=0.5,outlier.alpha=0.1,outlier.color="grey") +
stat_summary(color="black") +
facet_wrap(~data,nrow = 1, scale="free") + theme_bw() +
theme(axis.title.x=element_blank(),axis.text.x=element_blank(), axis.ticks.x=element_blank()) +
theme(legend.position = "bottom", legend.title = element_blank())
